,"source":"Containers/Inventory/Inventory.js","original":{"line":83,"column":5}},{"generated":{"line":79,"column":0},"source":"Containers/Inventory/Inventory.js","original":{"line":5,"column":34}},{"name":"Inventory","generated":{"line":81,"column":15},"source":"Containers/Inventory/Inventory.js","original":{"line":86,"column":15}}],"sources":{"Containers/Inventory/Inventory.js":"import React, { Component } from 'react';\r\nimport {Container,Row,Col,Card, ListGroup} from 'react-bootstrap';\r\nimport './Inventory.css'\r\n\r\nclass Inventory extends Component {\r\n    constructor(props){\r\n        super(props)\r\n        this.state={\r\n            userInv:[],\r\n            invName:[],\r\n            invQty:[]\r\n\r\n        }\r\n    }\r\n\r\n    UpdateInv=async()=>{\r\n        await window.contract.getInventory()\r\n        .then(res=>{\r\n            if(res!==undefined){\r\n            this.setState({\r\n                userInv:res  \r\n            })\r\n            }            \r\n            else{\r\n               return null\r\n            }\r\n        }\r\n        )\r\n    }\r\n\r\n    orgInv=()=>{\r\n        let names=[];\r\n        let qty=[];\r\n        this.state.userInv.forEach(\r\n            (x)=>{\r\n           \r\n                if(names.includes(x)){\r\n                    qty[names.indexOf(x)]=qty[names.indexOf(x)]+1\r\n                }\r\n                else{\r\n                    names.push(x);\r\n                    qty.push(1);\r\n                }\r\n            }\r\n        )\r\n       \r\n        this.setState(\r\n            {\r\n                invName:names,\r\n                invQty:qty\r\n            }\r\n        )\r\n    }\r\n\r\n\r\n\r\n\r\n    componentDidMount(){\r\n        (window.accountId!=='')? \r\n        this.UpdateInv():null;\r\n    }\r\n\r\n    componentDidUpdate(prevprops,prevstate){\r\n        if(prevstate.userInv!==this.state.userInv){\r\n        this.orgInv();\r\n        }\r\n        \r\n    }\r\n\r\n    //userInv=[{name:'Echo Pearl', Qty:8},{name:'Water Shield', Qty:10}]\r\n\r\n    render() {\r\n        return (\r\n            <div className=\" InventoryContainer d-flex justify-content-center\">\r\n                <Card style={{width:'70rem'}}>\r\n                <Card.Header>User's Inventory</Card.Header>\r\n                    <ListGroup>\r\n                        {this.state.invName.map((x,index)=>(<ListGroup.Item key={x}>{x} x {this.state.invQty[index]}</ListGroup.Item>))}\r\n                    </ListGroup>\r\n                 </Card>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Inventory;"},"lineCount":null}},"error":null,"hash":"092473a847fa0f0b053b8cb0c0054bb0","cacheData":{"env":{}}}